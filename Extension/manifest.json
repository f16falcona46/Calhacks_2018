{
    "manifest_version": 2,
    "name": "CalHacks2018",
    "version": "1",

    "content_scripts": [
        {   
            "matches": ["<all_urls>"],
            "js": ["jquery-3.3.1.min.js", "content.js"]
        }
    ],

    "browser_action": {
        "default_icon": "bread.png",
        "default_title": "icon",
        "default_popup": "popup.html"
    },

    "icons": {
        "128": "bread.png"
    },

    "background": {
        "scripts": ["background.js"]
    },

    "permissions": [
        "<all_urls>",
        "tabs"
    ]
}